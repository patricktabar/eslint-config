{
  "name": "@patricktabar/eslint-config-source",
  "author": "Patrick Tabarelli <patrick.tabarelli@gmail.com>",
  "license": "MIT",
  "version": "1.0.0",
  "private": false,
  "description": "A monorepo for personal, shareable ESlint configurations",
  "workspaces": [
    "packages/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/patricktabar/eslint-config",
  "bugs": {
    "url": "https://github.com/patricktabar/eslint-config/issues",
    "email": "patrick.tabarelli@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patricktabar/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "configuration"
  ],
  "scripts": {
    "build": "echo build",
    "dev": "echo dev",
    "lint": "turbo lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "change": "npx changeset",
    "version": "npx changeset version",
    "release": "npx changeset publish"
  },
  "devDependencies": {
    "@patricktabar/prettier-config": "^1.3.6",
    "@types/node": "^20.12.12",
    "eslint": "^9.3.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "packageManager": "pnpm@9.0.6",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "eslint": "^9.2.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.3"
  }
}
